<% for my $elem (@$fbcc) { =%>
<%=l 'Note' %> <%= $elem->{'at'} . ' ' %><%=l 'was the first from' =%>
 :flag-<%= $elem->{'imgname'} %>: [b]<%= $elem->{'cname'} %>[/b] ( :note-<%= $elem->{'value'} %>:<%=' '%><%=l 'entered on' =%>
<%= ' ' . $elem->{'on'} . ' ' %><%=l 'in' %> <%= $elem->{'city'} %> :flag-<%= $elem->{'imgname2'} %>:)
<% } =%>
