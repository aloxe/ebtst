<h2><%=l 'Hits by month' %></h2>
<table>
    <tr>
        <th></th>
        <th><%=l 'Natural month' %></th>
        <th><%=l 'Insert month' %></th>
    </tr>
    <% foreach my $row (@$rows) { %>
    <tr>
        <td><%= $row->{'month'} %></td>
        <td><%= $row->{'natural'} %> (<%= sprintf '%.2f%%', 100 * $row->{'natural'} / $n_hits %>)</td>
        <td><%= $row->{'insert'} %> (<%=  sprintf '%.2f%%', 100 * $row->{'insert'}  / $n_hits %>)</td>
    </tr>
    <% } %>
</table>
