<h2><%=l 'Number of notes per month' %></h2>
<% foreach my $elem (@$nbm) { %>
    <b><%= $elem->{'month'} %></b> - <%= $elem->{'count'} %> (<%= $elem->{'pct'} %>%) - 
    <% foreach my $d (@{ $elem->{'detail'} }) { %>
        <% if ($d->{'count'}) { %>
            [<%= $d->{'count'} %> <img src="images/<%= $d->{'value'} %>.gif">]
        <% } %>
    <% } %>
    <br>
<% } %>
