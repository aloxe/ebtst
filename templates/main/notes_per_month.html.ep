<h2><%=l 'Number of notes per month' %></h2>
<table>
    <tr>
        <th><%=l 'Month' %></th>
        <th><%=l 'Notes' %></th>
        <th><img src="images/values/5.gif"></th>
        <th><img src="images/values/10.gif"></th>
        <th><img src="images/values/20.gif"></th>
        <th><img src="images/values/50.gif"></th>
        <th><img src="images/values/100.gif"></th>
        <th><img src="images/values/200.gif"></th>
        <th><img src="images/values/500.gif"></th>
    </tr>
    <% foreach my $elem (@$nbm) { %>
        <tr>
            <td><b><%= $elem->{'month'} %></b></td>
            <td><%= $elem->{'count'} %> (<%= $elem->{'pct'} %>%)</td>
            <% foreach my $d (@{ $elem->{'detail'} }) { %>
                <% if ($d->{'count'}) { %>
                    <td><%= $d->{'count'} %></td>
                <% } else { %>
                    <td></td>
                <% } %>
            <% } %>
        </tr>
    <% } %>
</table>
