<h2><%=l 'Time analysis' %></h2>
<% no warnings 'qw'; use List::Util qw/sum/; %>
<% my %last_days = qw/01 31   02 29   03 31   04 30   05 31   06 30   07 31   08 31   09 30   10 31   11 30   12 31/; %>
<% my %color_counts; =%>

<b><%=l 'Calendar' %></b><br>
<table>
<% foreach my $m (map { sprintf '%02d', $_ } 1 .. 12) { %>
    <tr>
        <th class="small_cell"><%=l $EBTST::Main::months{0+$m} %></th>
        <% foreach my $d (map { sprintf '%02d', $_ } 1 .. $last_days{$m}) { %>
            <% my $color = color $ta->{'cal'}{$m}{$d}; $color_counts{$color}++; %>
            <td class="small_cell"><span style="color: <%= $color %>"><%= $d %></span></td>
        <% } %>
    </tr>
<% } %>
</table>
<b><%=l 'Colors' %></b>:
<span style="color: <%= color    1 %>"><%= sum map { $color_counts{$_}//0 } qw/blue green #FFBF00 red black/ %>/366</span>,
<span style="color: <%= color   51 %>"><%= sum map { $color_counts{$_}//0 } qw/     green #FFBF00 red black/ %>/366</span>,
<span style="color: <%= color  101 %>"><%= sum map { $color_counts{$_}//0 } qw/           #FFBF00 red black/ %>/366</span>,
<span style="color: <%= color  501 %>"><%= sum map { $color_counts{$_}//0 } qw/                   red black/ %>/366</span>,
<span style="color: <%= color 1001 %>"><%= sum map { $color_counts{$_}//0 } qw/                       black/ %>/366</span><br>
<br><br>

<% %color_counts = (); =%>
<table>
    <tr>
        <th></th>
        <th><b><%=l 'Hours' %></b></th>
    </tr>
    <% foreach my $num (map { sprintf '%02d', $_ } 0 .. 23) { =%>
        <tr>
            <th><%= $num %></th>
            <% my $color = color $ta->{'hh'}{$num}; $color_counts{$color}++; %>
            <td><span style="color: <%= $color %>"><%= $ta->{'hh'}{$num} %></span> (<%= sprintf '%.2f', 100 * $ta->{'hh'}{$num} / $count %>%)</td>
        </tr>
    <% } %>
</table>
<b><%=l 'Colors' %></b>:
<span style="color: <%= color    1 %>"><%= sum map { $color_counts{$_}//0 } qw/blue green #FFBF00 red black/ %>/24</span>,
<span style="color: <%= color   51 %>"><%= sum map { $color_counts{$_}//0 } qw/     green #FFBF00 red black/ %>/24</span>,
<span style="color: <%= color  101 %>"><%= sum map { $color_counts{$_}//0 } qw/           #FFBF00 red black/ %>/24</span>,
<span style="color: <%= color  501 %>"><%= sum map { $color_counts{$_}//0 } qw/                   red black/ %>/24</span>,
<span style="color: <%= color 1001 %>"><%= sum map { $color_counts{$_}//0 } qw/                       black/ %>/24</span><br>
<br><br>

<% %color_counts = (); =%>
<b><%=l 'Minutes' %></b><br>
